<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>es.cice.login.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Publicacion</servlet-name>
        <servlet-class>es.cice.models.Publicacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuario</servlet-name>
        <servlet-class>es.cice.models.Usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Amigo</servlet-name>
        <servlet-class>es.cice.models.Amigo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BDManager</servlet-name>
        <servlet-class>es.cice.controller.BDManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Manager</servlet-name>
        <servlet-class>es.cice.controller.Manager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WordManager</servlet-name>
        <servlet-class>es.cice.controller.WordManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>es.cice.login.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Publicacion</servlet-name>
        <url-pattern>/Publicacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuario</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Amigo</servlet-name>
        <url-pattern>/Amigo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BDManager</servlet-name>
        <url-pattern>/BDManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Manager</servlet-name>
        <url-pattern>/Manager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
